       <Tracking event="exitFullscreen"><![CDATA[{exitFullscreen}]]></Tracking>
       <Tracking event="acceptInvitationLinear"><![CDATA[{acceptInvitationLinear}]]></Tracking>
       <Tracking event="closeLinear"><![CDATA[{closeLinear}]]></Tracking>
       <Tracking event="skip"><![CDATA[{skip}]]></Tracking>
       <Tracking event="progress" offset="{skipoffset}"><![CDATA[{video_skip_shown}]]></Tracking>
       <Tracking event="progress" offset="{duration}"><![CDATA[{video_engaged_view}]]></Tracking>
      </TrackingEvents>
      <VideoClicks>
       <ClickThrough id="GDFP"><![CDATA[{ClickThrough}]]></ClickThrough>
      </VideoClicks>
      <MediaFiles>
       {mediafile}
      </MediaFiles>
